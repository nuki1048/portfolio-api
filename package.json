    "mailgen": "^2.0.28",
    "mongoose": "^8.0.2",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.7",
    "octokit": "^3.1.2",
    "slugify": "^1.6.6",
    "ts-jest": "^29.1.1"
